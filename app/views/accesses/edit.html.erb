<% content_for :page_title, "My Projects" %>

<br/>
         <div class="message notice">
<p>OriginalProjects notifies you whenever actions take place related to projects you are involved in.</p><br/>
<p>Each project has its own notification settings.</p><br/>
<p>Check the boxes for each notification you'd like to receive at <%=current_profile.email%>.</p><br/></div>
<div id="content">
     <h3> <%= link_to "#{@access.project.title}",project_path(@access.project)%></h3>
                <%#*<div class="categories rounded originated projects">%>
 <div class="project_row">
<% form_for :access ,:url=>{:conroller=>"accesses",:action=>"update"},:html=>{:method=>:put}do |f| %>
     <%= f.error_messages %>
                
  <%=render :partial=>'form',:locals=>{:f=>f}%>
<br/>
   <p style="padding-left: 11px; margin-top: 10px;"><button class="large green" type="submit">Save Changes</button></p>
       </div>
   
      </div>

<% end %>




